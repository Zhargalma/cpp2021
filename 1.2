#include <iostream>

using namespace std;

int main()
{
    int b;
    float d;
    char v;
    short n;
    float s, m, e, f;
    cin >> b >> d >> v>> n;
    s = b + d;
    m = v / n;
    e = d * v;
    f = b - n;
    cout << "sum=" << s << endl;
    cout << "division=" << m << endl;
    cout << "multi=" << e << endl;
    cout << "difference=" << f << endl;

    return 0;
}
