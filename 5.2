#include <iostream>

using namespace std;

void Sort(int massive[])
{
  for (int i=0; i<10-1; i++)
  {
    for (int z=i+1; z<10; z++)
    {
      if (massive[i]<massive[z])
      {
        int a=massive[z];
        massive[z]=massive[i];
        massive[i]=a;
      }
    }
  }
}

int main()
{
  int massive[10];
  cout<<"Введите массив из 10 чисел"<<'\n';
  for (int i=0; i<10; i++)
  {
    cout<<" ";
    cin>>massive[i];
  }
  Sort(massive);
  for (int z=0; z<10; z++)
  {
    cout<<massive[z]<<' ';
  }
  cout<<'\n';
  return 0;
}
