#include<fstream> // подключаем библиотеку "fstream"
#include<iostream> // подключаем библиотеку "iostream"
using namespace std; // используем пространство имён std
int main() // вызов фунуции main, int задаёт тип данного,возращаемого функцией main
{ // начало тела функции main
    int a, b; // объявляем переменные a и b
	ofstream f; // создаём объект класса ofstream(для вывода данных)
	f.open ("output");   // открываем файл и задаём имя 
	cout<<" a = "; // выводим переменную a на экран
	cin>>a; // вводим значение a с клавиатуры
	cout<<" b = "; //выводим переменную b на экран
	cin>>b;//вводим значение b с клавиатуры
	 for (int i = a; i <= b ; i++)// задаём цикл for, где "int i = a"-объявление счетчика,создаём первоначальное значение,"i <= b"-условие выполнения цикла,т.е. i меньше или равно b,"i++"-шаг цикла,т.е. i увеличивается на один
    { //начало тела цикла
        f << i * 3 << endl; // в файле каждое значение цикла умножается на 3
    }// окончание цикла
    f.close ( ); // закрываем открытый файл 
return 0; // завершение функции main, возвращение значения 0
} // завершение программы
